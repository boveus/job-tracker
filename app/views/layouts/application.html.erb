<!DOCTYPE html>
<html>
<header class=eqi-container>
  <%= link_to 'All Companies', :companies %>
  <%= link_to 'Jobs Dashboard', :dashboard %>
  <%= link_to 'All jobs', :all_jobs %>
  <%= link_to 'All job categories', :categories %>
</header>

<head>
  <title>JobTracker</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash.each do |name, msg| %>
    <%= content_tag :div, msg, class: name %>
  <% end %>
<%= yield %>

</body>


</html>
