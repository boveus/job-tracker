<h3>All Jobs for <strong><%= @company.name %></strong> </h3>
<ul>
<% @company.jobs.each do |job| %>
    <li class=job_<%= job.id %>> <%= link_to "#{job.title}", company_job_path(job.company, job) %></li>
<% end %>
</ul>

<% @company.contacts.each do |contact| %>
  <ul>Name: <%= contact.name %></ul>
  <ul>Position: <%= contact.position %></ul>
  <ul>E-mail: <%= contact.email %></ul>
<% end %>

<%= render partial: "contact_form" %>
