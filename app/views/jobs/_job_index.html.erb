<% @jobs.each do |job| %>
  <ul class=job_<%= job.id %>> <%= link_to "#{job.title} at #{job.company.name}", company_job_path(job.company, job) %>
    <li> Job location: <%= job.city %> </li>
    <li>
    Modify Job:
  <%= link_to "Edit", edit_company_job_path(job.company, job) %>
  <%= link_to "Delete", company_job_path(job.company, job), method: "delete" %>
    </li>
  </ul>
<% end %>
